# Environment variables for Housing.com Voice Agent

# SerpAPI key for Google search
SERPER_API_KEY=your_serper_api_key

# OpenAI API key for AI conversation (optional - will use fallback responses if not provided)
OPENAI_API_KEY=your_openai_api_key

# Twilio credentials (get these from Twilio Console)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Base URL for webhooks (update to your ngrok/deployed URL for production)
BASE_URL=http://localhost:3001

# Server port
PORT=3001